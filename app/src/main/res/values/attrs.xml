<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeRevealLayout">
        <attr name="dragEdge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
            <flag name="top" value="4" />
            <flag name="bottom" value="8" />
        </attr>

        <attr name="mode">
            <enum name="normal" value="0" />
            <enum name="same_level" value="1" />
        </attr>

        <attr name="flingVelocity" format="integer" />

        <attr name="minDistRequestDisallowParent" format="dimension" />
    </declare-styleable>


    <attr name="enabled" format="boolean" />
    <attr name="forceAspectRatio" format="boolean" />


    <attr name="direction">
        <enum name="up" value="0" />
        <enum name="right" value="1" />
        <enum name="down" value="2" />
        <enum name="left" value="3" />
    </attr>

    <attr name="scaleType">
        <enum name="matrix" value="0" />
        <enum name="fitXy" value="1" />
        <enum name="fitStart" value="2" />
        <enum name="fitCenter" value="3" />
        <enum name="fitEnd" value="4" />
        <enum name="center" value="5" />
        <enum name="centerCrop" value="6" />
        <enum name="centerInside" value="7" />
    </attr>

</resources>
